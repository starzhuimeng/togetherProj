<view><view class="content"><view class="form-item"><label>昵    称:</label><input type="text" maxlength="16" value="{{uname}}" placeholder="请输入您的称呼" data-event-opts="{{[['input',[['__set_model',['','uname','$event',[]]]]]]}}" bindinput="__e"/></view><view class="form-item"><label>手机号:</label><input id="pass" type="number" maxlength="11" value="{{phone}}" placeholder="请输入您的手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" bindinput="__e"/><view class="vcodebtn" id data-event-opts="{{[['tap',[['putPhoneCode',['$event']]]]]}}" bindtap="__e">{{''+phoneCodeVal+''}}</view></view><view class="form-item"><label>验证码:</label><input class="phone" type="number" maxlength="6" value="{{phonecode}}" placeholder="请输入获取的验证码" data-event-opts="{{[['input',[['__set_model',['','phonecode','$event',[]]]]]]}}" bindinput="__e"/></view><view class="form-item"><label>密    码:</label><input class="phone" type="text" value="{{pass}}" placeholder="请输入您的密码" data-event-opts="{{[['input',[['__set_model',['','pass','$event',[]]]]]]}}" bindinput="__e"/></view><view class="form-item"><label>验证码:</label><input type="text" value="{{vcode}}" placeholder="请输入图上的验证码" data-event-opts="{{[['input',[['__set_model',['','vcode','$event',[]]]]]]}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['changeVcode',['$event']]]]]}}" class="vcode" bindtap="__e"><image style="width:100%;height:100%;" src="{{vcodeUrl}}"></image></view></view></view><view class="btn"><button type="primary" data-event-opts="{{[['tap',[['complate',['$event']]]]]}}" bindtap="__e">提交</button></view></view>